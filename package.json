{
  "name": "hostel",
  "version": "0.9.0",
  "description": "Updated version of abandoned project 'hostel'.",
  "main": "lib",
  "bin": "lib/cli/bin.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "ava && run-s lint",
    "lint": "eslint . --ignore-path .gitignore && stylelint './src/app/**/*.css'",
    "fix": "eslint . --ignore-path .gitignore --fix && stylelint './src/app/**/*.css' --fix",
    "format": "run-s format:sort-imports format:prettier",
    "format:prettier": "prettier --loglevel warn --single-quote --write './src/**/*.{js,jsx,ts,tsx,json,scss,css}'",
    "format:sort-imports": "import-sort --write './src/**/*.{js,jsx,ts,tsx}'",
    "format:root": "prettier --loglevel warn --single-quote --write './*.{js,jsx,ts,tsx,json}'",
    "start": "run-p start:*",
    "start:webpack": "webpack-cli serve --mode development --config webpack.dev.js --open",
    "start:nodemon": "nodemon -- src/daemon",
    "prepublishOnly": "run-s build && pkg-ok",
    "uninstall": "node bin/uninstall.js",
    "build": "run-s build:*",
    "build:webpack": "rimraf dist && webpack --config webpack.prod.js",
    "build:babel": "rimraf lib && babel src -d lib --copy-files --ignore src/app",
    "addFixtures": "node src/cli/bin add \"node index\" --dir ./test/fixtures/app && node src/cli/bin add \"node index\" --dir ./test/fixtures/verbose && node src/cli/bin add http://some-domain.com --name local-domain "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hwknsj/hostel.git"
  },
  "keywords": [
    "dev",
    "devtool",
    "domain",
    "host",
    "https",
    "local",
    "localhost",
    "manager",
    "process",
    "proxy",
    "server"
  ],
  "author": "jo√©l hawkins torres <joel@joel.fm> (https://joel.fm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hwknsj/hostel/issues"
  },
  "homepage": "https://github.com/hwknsj/hostel",
  "contributors": [
    "Typicode <typicode@gmail.com"
  ],
  "dependencies": {
    "@webpack-cli/init": "^1.1.3",
    "after-all": "^2.0.2",
    "ansi2html": "0.0.1",
    "chalk": "^2.4.2",
    "chokidar": "^3.5.1",
    "connect-sse": "^1.2.0",
    "exit-hook": "^1.1.1",
    "express": "^4.17.1",
    "get-port": "^3.2.0",
    "http-proxy": "^1.18.1",
    "matcher": "^1.1.1",
    "mkdirp": "^1.0.4",
    "once": "^1.4.0",
    "please-upgrade-node": "^3.2.0",
    "pug": "^2.0.4",
    "respawn": "^2.6.0",
    "selfsigned": "^1.10.8",
    "server-ready": "^0.3.1",
    "sudo-block": "^2.0.0",
    "tildify": "^1.1.2",
    "tinydate": "^1.3.0",
    "unquote": "^1.1.1",
    "untildify": "^3.0.2",
    "update-notifier": "^2.3.0",
    "user-startup": "^0.2.2",
    "vhost": "^3.0.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-typescript": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.17",
    "@types/classnames": "^2.2.11",
    "@types/escape-html": "1.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^16.9.11",
    "@types/react-icons": "^2.2.7",
    "ava": "^3.15.0",
    "classnames": "^2.2.6",
    "css-loader": "^0.28.10",
    "escape-html": "^1.0.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^0.15.0-rc.13",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "lodash.uniqueid": "^4.0.1",
    "mobx": "^5.15.7",
    "mobx-react": "^5.4.4",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "pkg-ok": "^2.3.1",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^2.2.7",
    "rimraf": "^3.0.2",
    "sinon": "^4.5.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "supertest": "^3.4.2",
    "tempy": "^0.2.1",
    "ts-loader": "^8.0.17",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.1.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "ava": {
    "serial": true,
    "verbose": true,
    "require": [
      "babel-register",
      "./test/_setup"
    ],
    "babel": "inherit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format format:root"
    }
  }
}
